{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "typeRoots": ["node_modules/@types", "typings/app.d.ts", "node_modules/handlebars/types/index.d.ts"], 
    "sourceMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}
